{
    "name": "bitaac/bitaac",
    "description": "An automatic account creator for Open Tibia servers.",
    "keywords": ["bitaac", "bitfabric", "laravel", "tibia", "opentibia"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "eklundkristoffer/seedster": "^1.0",
        "bitaac/theme-retro": "1.0.*",
        "bitaac/theme-admin": "1.0.*",
        "pragmarx/google2fa": "^1.0",
        "bacon/bacon-qr-code": "~1.0",
        "omnipay/paypal": "~2.0",
        "barryvdh/laravel-omnipay": "0.2.*@dev",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
